NODE_ENV=development

APP_NAME="New Saas Boilerplate"
APP_DESCRIPTION="Boilerplate application"

PORT=3000
HOST=0.0.0.0
PROTOCOL=http

# CORS
CORS_ORIGINS=http://localhost:3001,http://127.0.0.1:3001

# Database
DATABASE_URL=postgresql://postgres:12345678@localhost:5432/saas-boilerplate

# Redis
REDIS_HOST=127.0.0.1
REDIS_PORT=6379
REDIS_PASSWORD=

# Better Auth
BETTER_AUTH_SECRET="test-secret-key-change-in-production"
